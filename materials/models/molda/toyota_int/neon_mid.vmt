"VertexLitGeneric"
{
	"$basetexture" "models/molda/toyota_int/neon_in"
	"$surfaceprop" "Metal"

	"$selfillum" 1
	"$selfillummask" "/models/molda/toyota_int/neon_in"

	"$nodecal" "1"
	"$nocull" "0"

	$rotor_mult "1"
	$rotor_color "[1 1 1]"
	$rotor "[1 1 1]"
	$rotor_nopower "[0 0.2 0.5]"
	Proxies {
		TARDIS_DefaultInt_RotorInColor {
			resultVar	$rotor_color
		}
		Multiply {
			srcVar1 $rotor_color
			srcVar2 $rotor_mult
			resultVar $rotor
		}
		TARDIS_Power {
			onVar "$rotor"
			offVar "$rotor_nopower"
			resultVar "$color2"
		}
	}
}
